    key <AE10> { [ 0,            parenright,  NoSymbol,     NoSymbol     ] };
    key <AE11> { [ NoSymbol,     NoSymbol,    NoSymbol,     NoSymbol     ] };
    key <AE12> { [ NoSymbol,     NoSymbol,    NoSymbol,     NoSymbol     ] };
    
    // Первый буквенный ряд (QWERTY)
    key <AD01> { [ q,            Q,           exclam,       NoSymbol     ] };
    key <AD02> { [ w,            W,           at,           NoSymbol     ] };
    key <AD03> { [ e,            E,           numbersign,   NoSymbol     ] };
