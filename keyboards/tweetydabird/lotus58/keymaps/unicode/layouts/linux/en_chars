    key <AB06> { [ n,            N,           less,         NoSymbol     ] };
    key <AB07> { [ m,            M,           greater,      NoSymbol     ] };       
    key <AB08> { [ F23,     NoSymbol,    comma,        NoSymbol     ] };
    key <AB09> { [ F22,     NoSymbol,    period,       NoSymbol     ] };
