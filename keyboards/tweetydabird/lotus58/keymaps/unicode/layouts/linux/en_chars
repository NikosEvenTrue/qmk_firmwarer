    key <AD05> { [ t,            T,           percent,      NoSymbol     ] };
    key <AD06> { [ y,            Y,           braceleft,    NoSymbol     ] };
    key <AD07> { [ u,            U,           parenleft,    NoSymbol     ] };
    key <AD08> { [ i,            I,           parenright,   NoSymbol     ] };
    key <AD09> { [ o,            O,           bracketleft,  NoSymbol     ] };
    key <AD10> { [ p,            P,           bracketright, NoSymbol     ] };
    key <AD11> { [ NoSymbol,     NoSymbol,    asciitilde,   NoSymbol     ] };
    key <AD12> { [ NoSymbol,     NoSymbol,    NoSymbol,     NoSymbol     ] };
    
    // Второй буквенный ряд (ASDF)
    key <AC01> { [ a,            A,           plus,         NoSymbol     ] };
    key <AC02> { [ s,            S,           minus,        NoSymbol     ] };
    key <AC03> { [ d,            D,           asterisk,     NoSymbol     ] };
